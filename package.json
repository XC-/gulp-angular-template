{
  "name": "gulp-angular-template",
  "version": "1.0.1",
  "description": "Project template for gulp.js",
  "author": "Aki MÃ¤kinen <aki.makinen@outlook.com>",
  "contributors": "Riku Rouvila <riku.rouvila@leonidasoy.fi>",
  "license": "MIT",
  "main": "gulpfile.js",
  "scripts": {
    "start": "rm -rf public && gulp",
    "build": "rm -rf public && gulp build",
    "lint": "eslint src",
    "test": "mocha src/**/__tests__/*.js --compilers js:babel-core/register --require test/test-helper"
  },
  "keywords": [
    "gulp",
    "template"
  ],
  "dependencies": {
    "angular": "^1.5.0",
    "angular-route": "^1.5.0",
    "lodash": "^4.0.0"
  },
  "build": {
    "source":"./src",
    "destination":"./public",
    "scripts":{
      "source":"./src/app.js",
      "destination":"./public/js/",
      "extensions":[],
      "filename":"bundle.js"
    },
    "templates":{
      "source": [
        "./src/**/*.jade"
      ],
      "watch":"./src/**/*.jade",
      "destination":"./public/",
      "revision":"./public/views/**/*.html"
    },
    "styles":{
      "source":"./src/styles/**/*.styl",
      "watch":"./src/styles/**/*.styl",
      "destination":"./public/css/",
      "filename":"style.css",
      "browserVersions":[
        "last 2 versions",
        "Chrome 34",
        "Firefox 28",
        "iOS 7"
      ]
    },
    "assets":{
      "source":"./src/assets/**/*.*",
      "watch":"./src/assets/**/*.*",
      "destination":"./public/"
    },
    "inject":{
      "resources":[
        "./public/css/*.css",
        "./public/**/*.js"
      ]
    }
  },
  "devDependencies": {
    "babel": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "chai": "^3.5.0",
    "envify": "^3.4.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "0.0.9",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-duration": "0.0.0",
    "gulp-inject": "~3.0.0",
    "gulp-jade": "~1.1.0",
    "gulp-replace": "^0.5.3",
    "gulp-rev": "^7.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.0",
    "gulp-stylus": "~2.3.0",
    "gulp-uglify": "~1.5.0",
    "gulp-util": "~3.0.1",
    "gulp-watch": "^4.3.4",
    "jsdom": "^8.0.0",
    "mocha": "^2.4.0",
    "node-notifier": "^4.5.0",
    "rimraf": "^2.5.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ],
      [
        "envify"
      ]
    ]
  }
}
